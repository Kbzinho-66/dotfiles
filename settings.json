    "vim.normalModeKeyBindings": [
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "<Leader>",
                "o"
            ],
            "after": [
                "o",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<Leader>",
                "O"
            ],
            "after": [
                "O",
                "<Esc>"
            ]
        },
        {
            "before": [
                "<Leader>",
                "n"
            ],
            "after": [
                "g",
                "g",
                "n"
            ]
        },
        {
            "before": [
                "<Leader>",
                "N"
            ],
            "after": [
                "G",
                "N"
            ]
        },
        {
            "before": [
                "<Leader>",
                "]"
            ],
            "after": [
                "c",
                "s",
                "\"",
                "]"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-w>",
                "j"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-w>",
                "k"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "f"
            ],
            "commands": [
                "revealInExplorer"
            ],
        },
        {
            "before": [
                "<Leader>",
                "1"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<Leader>",
                "2"
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "<Leader>",
                "3"
            ],
            "commands": [
                "workbench.action.showCommands"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<Leader>",
                "+"
            ],
            "commands": [
                "editor.action.transformToUppercase"
            ]
        },
        {
            "before": [
                "<Leader>",
                "-"
            ],
            "commands": [
                "editor.action.transformToLowercase"
            ]
        },
        {
            "before": [
                "<Leader>",
                "<"
            ],
            "commands": [
                "editor.action.sortLinesAscending"
            ]

        },
        {
            "before": [
                "<Leader>",
                ">"
            ],
            "commands": [
                "editor.action.sortLinesDescending"
            ]

        }
    ],
